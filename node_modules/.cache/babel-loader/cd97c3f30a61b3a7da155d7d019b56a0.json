00 }\"\r\n        loop\r\n        effect=\"fade\"\r\n      >\r\n        <swiper-slide :key=\"image\" v-for=\"image in product.images\"\r\n          ><img :src=\"require(`@/assets/img/${image}`)\" alt=\"\"\r\n        /></swiper-slide>\r\n      </swiper>\r\n    </div>\r\n    <div class=\"item-right\">\r\n      <div class=\"item-text\">\r\n        <h3>{{ product.name }}</h3>\r\n        <p>{{ product.description }}</p>\r\n        <p>{{ product.materials }}</p>\r\n        <p>{{ product.dimensions }}</p>\r\n        <div class=\"price\">{{ product.price }} грн</div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n  <h3 v-if=\"product.similar\">Схожі товари</h3>\r\n  <div class=\"product-similar\">\r\n    <PillowsSimilarProduct v-for=\"item in product.similar\" :item=\"item\" />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport pillows from '../data/pillows.js';\r\nimport PillowsSimilarProduct from '../components/products/PillowsSimilarProduct';\r\nimport { Swiper, SwiperSlide } from 'swiper/vue';\r\nimport SwiperCore, { Pagination, Autoplay, EffectFade } from 'swiper';\r\n\r\nSwiperCore.use([Pagination, Autoplay, EffectFade]);\r\n\r\nexport default {\r\n  components: {\r\n    Swiper,\r\n    SwiperSlide,\r\n    Pagination,\r\n    Autoplay,\r\n    PillowsSimilarProduct\r\n  },\r\n  data() {\r\n    return {\r\n      product: null,\r\n      pillows: pillows\r\n    };\r\n  },\r\n  created() {\r\n    const product = pillows.find(\r\n      product => product.link === this.$route.params.link\r\n    );\r\n    if (product) {\r\n      this.product = product;\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style></style>\r\n"],"sourceRoot":""}]}